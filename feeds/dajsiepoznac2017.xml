---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{site.title}} {{category}}</title>
    <description>Wpisy z kategorii Daj się poznać</description>
    <link>{{ site.url }}</link>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    {% for post in site.categories.dajsiepoznac2017 %}
      {% unless post.draft %}
        <item>
          <title>{{ post.title | xml_escape }}</title>
          <description>{{ post.excerpt | default: '' | strip_html | normalize_whitespace | truncate: 200 | escape }}</description>
          <link>{{ post.url | prepend: site.url }}</link>
          <guid>{{ post.url | prepend: site.url }}</guid>
        </item>
      {% endunless %}
    {% endfor %}
  </channel>
</rss>
